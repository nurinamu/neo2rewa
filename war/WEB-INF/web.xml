	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Neo2rewa</servlet-name>
		<servlet-class>com.veckon.hack.neo2rewa.Neo2rewaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Neo2rewa</servlet-name>
		<url-pattern>/neo2rewa</url-pattern>
	</servlet-mapping>
 	<filter>
		<filter-name>ObjectifyFilter</filter-name>
		<filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ObjectifyFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

